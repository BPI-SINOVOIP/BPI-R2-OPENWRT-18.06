Source-Makefile: target/linux/ar7/Makefile
Target: ar7
Target-Board: ar7
Target-Name: TI AR7
Target-Arch: mipsel
Target-Arch-Packages: mipsel_mips32
Target-Features: atm gpio low_mem squashfs
Target-Depends: 
Target-Optimization: -Os -pipe -mno-branch-likely -mips32 -mtune=mips32
CPU-Type: mips32
Linux-Version: 4.9.155
Linux-Release: 1
Linux-Kernel-Arch: mips
Target-Description:
	Build firmware images for TI AR7 based routers.
@@
